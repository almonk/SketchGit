// Checkout (cmd alt ctrl c)

#import 'library/common.js'

var path = net.khasegawa.getCurrentDirectory(),
    name = doc.askForUserInput_initialValue_('Checkout branch', '');
if (name != null)
{
  var command = NSString.stringWithFormat_("\\cd \"%@\" && \\git checkout -q %@", path, name);
  net.khasegawa.exec(command);
}
